  "bugs": {
    "url": "https://github.com/christian-bromann/webdriverio-applitools-testing/issues"
  },
  "homepage": "https://github.com/christian-bromann/webdriverio-applitools-testing#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@wdio/applitools-service": "^5.4.4",
    "@wdio/cli": "^5.4.0",
    "@wdio/eyes.webdriverio": "^2.0.1",
    "@wdio/local-runner": "^5.4.0",
    "@wdio/mocha-framework": "^5.4.0",
